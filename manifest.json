{
  "name": "Ad-Finder",
  "description": "Finds Advertisement related JavaScript",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Ad-Finder"
  },
  "manifest_version": 2,
  
  "browser_action": {
    "default_icon": "adblock.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  }
}
